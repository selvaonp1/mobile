<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="@drawable/llp"
    tools:context="com.location.sunday1.society.ServiceList">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#4000"
        android:alpha="1.2"
        ></RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="15dp">

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/listView11"
        android:layout_alignParentLeft="true"
        android:background="#01A3DA"
        android:layout_marginTop="30dp"
        android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="EmBuddy / "
            android:textColor="#fff"
            android:id="@+id/textView93"
            android:textSize="12dp"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Medium Text"
            android:id="@+id/textView94"
            android:textColor="#fff"
            android:textSize="12dp"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/textView93"
            android:layout_toEndOf="@+id/textView93" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Medium Text"
            android:textColor="#fff"
            android:textSize="12dp"
            android:id="@+id/textView101"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/textView94"
            android:layout_toEndOf="@+id/textView94" />
    </RelativeLayout>
</RelativeLayout>
